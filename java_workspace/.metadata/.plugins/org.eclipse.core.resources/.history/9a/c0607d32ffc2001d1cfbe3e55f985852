package Prouduct;

public class Order extends Product {
	
	//2. Order class extends Product => 수량, total  (선택)
	
	private int count;
	private int total;
	
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	
	public void print() {
		//System.out.println(super.getMenuName()+" : "+getPrice());
		System.out.println(count+"개 주문 => 금액 : "+total+"원");
	}

}
