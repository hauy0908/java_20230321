package day17;

import java.io.IOException;

public class SystemInEX01 {

	public static void main(String[] args) {
		/* 알파벳 하나를 쓰고 Enter를 누르면 알파벳을 콘솔에 출력
		 * 
		 */
		System.out.println("알파넷 하나를 쓰고 Enter : ");
		int i;
		try {
			i = System.in.read();
		} catch (IOException e) {
			e.printStackTrace();
		}

	}

}
